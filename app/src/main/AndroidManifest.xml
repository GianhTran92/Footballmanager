<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="vn.asiantech.intership.myapplication" >

    <application
        android:name=".BaseApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".ui.league.LeagueActivity_"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity_"
            android:label="@string/title_activity_league" >
        </activity>
        <activity
            android:name=".ui.FootballTeam.FootballTeamActivity_"
            android:label="@string/title_activity_football_team" >
        </activity>
        <activity
            android:name=".ui.player.PlayerActivity_"
            android:label="@string/title_activity_player" >
        </activity>
        <activity
            android:name=".ui.player.NewPlayerActivity"
            android:label="@string/title_activity_new_player" >
        </activity>
    </application>

</manifest>
